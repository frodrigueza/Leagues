
	<table class="table">
		<thead>
			<tr>
				<th>Jugador</th>
				<th>Equipo</th>
				<%= render 'layouts/goals_cards_theaders' %>
			</tr>
		</thead>
		<% @participations_stads.each do |p| %>
			<tr>
				<td><%= link_to p[:participation].user.f_name, version_participation_path(@version, p[:participation]) %></td>
				<td><%= link_to p[:participation].inscription.team.name, version_inscription_path(@version, p[:participation].inscription) %></td>
				<td><%= p[:goals] %></td>
				<td><%= p[:yellows] %></td>
				<td><%= p[:reds] %></td>
			</tr>
		<% end %>
	</table>