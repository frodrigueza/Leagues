<h1><%= @league.name %></h1>
<h2>Copas</h2>

<div class="row">
	<div class="col-md-12">
		<table class="table">
			<thead>
				<th>
					Nombre copa
				</th>
				<th>
					Equipos
				</th>
			</thead>
			<% @league.cups.each do |cup| %>
				<tr>
					<td><%= cup.name %></td>
					<td><%= cup.versions.last.inscriptions.count %></td>
				</tr>
			<% end %>
		</table>
	</div>
</div>