<%= render layout:'versions/version' do %>

  <h1>Equipos inscritos en <%= @version.name %></h1>

  <div class="row">
    <div class="col-md-12">
      <table>
        <thead>
          <tr>
            <th>Nombre del equipo</th>
          </tr>
        </thead>

        <% @inscriptions.sort_by{|x| x.team.name }.each do |i| %>
          <tr>
            <td><%= link_to i.team.name, version_inscription_path(@version, i) %></td>
          </tr>
        <% end %>
      </table>
    </div>    
  </div>

<% end %>