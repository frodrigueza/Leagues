<%= render layout:'versions/version' do %>

<div class="row">
  <div class="col-md-12">
    <div class="page-header">
      <h1>Equipos inscritos en <%= @version.name %></h1>
  </div>
</div>


  <div class="row">
    <div class="col-md-12">
      <ol>
        <% @inscriptions.sort_by{|x| x.team.name }.each do |i| %>
            <li><%= link_to i.team.name, version_inscription_path(@version, i) %></li>
        <% end %>
      </ol>
<% end %>