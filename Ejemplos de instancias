	name: Apertura 2015
	cup_id

Inscription
	version_id
	team_id
	# Es la inscripcion de un equipo en la version de una copa.

Participation
	inscription_id
	user_id
	# Es la participacion de un jugador en la inscripcion de un equipo en una version de copa

